{
    "name": "granola",
    "version": "1.0.0",
    "description": "webapp to compare granola prices across several supermarkets",
    "main": "index.js",
    "scripts": {
        "run": "webpack-dev-server --config ./config/webpack.config.js",
        "webpack": "webpack --config ./config/webpack.config.js",
        "build": "(tsc -p tsconfig.json && robocopy ./config/ ./build/ template.yaml >nul 2>&1)",
        "test": "(cd build && sam local invoke GranolaScraperFunction --event ../data/schedule.json)"
    },
