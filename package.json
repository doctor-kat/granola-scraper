        "cheerio": "^1.0.0-rc.2",
        "npm": "^6.1.0",
        "puppeteer": "^1.3.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.7",
        "@types/node": "^6.0.110",
        "@types/puppeteer": "^1.3.2",
        "awesome-typescript-loader": "^5.0.0",
        "eslint-config-google": "^0.9.1",
        "ts-loader": "^4.3.0",
        "typescript": "^2.8.3",
        "webpack": "^4.8.2",
        "webpack-cli": "^2.1.4",
        "webpack-dev-server": "^3.1.4"
    }
